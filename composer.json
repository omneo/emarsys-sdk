{
    "name": "omneo/emarsys-sdk",
    "description": "PHP SDK for the Emarsys API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arkade\\Emarsys\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arkade\\Emarsys\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.2",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9"
    },
    "config": {
        "sort-packages": true
    }
}
